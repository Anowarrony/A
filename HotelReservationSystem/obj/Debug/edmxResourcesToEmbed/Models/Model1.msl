          <ScalarProperty Name="Delux" ColumnName="Delux" />
          <ScalarProperty Name="Indate" ColumnName="Indate" />
          <ScalarProperty Name="Outdate" ColumnName="Outdate" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>